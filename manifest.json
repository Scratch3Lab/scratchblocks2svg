{
    "name": "Scratchblocks2svg",
    "version": "0.1.0",
    "manifest_version": 2,
    "icons": {
        "128": "icon.png"
    },
    "permissions": [
        "storage",
        "activeTab"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "content_scripts": [
        {
          "matches": ["*://*/*"],
          "js": ["contentscript.js"],
          "run_at": "document_end"
        }
    ],
    "description": "将scratchblock导出为svg文件",
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "导出为svg文件",
        "default_popup": "index.html"
    }
}